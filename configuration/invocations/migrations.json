{
    "migration.get-config-registry": {
        "service": "services.core.configuration.manager",
        "method": "getConfigRegistry"
    },
    "migration.config.migrations": {
        "service": "invocations.migration.get-config-registry",
        "method": "get",
        "parameters": {
            "key": "migrations"
        }
    },
    "migration.grouped.migrations": {
        "service": "services.migration.grouper",
        "method": "__invoke",
        "parameters": {
            "migrations": "@{invocations.migration.config.migrations}"
        }
    },
    "migration.matrix": {
        "service": "services.version.matrix.generator",
        "method": "__invoke",
        "parameters": {
            "groupedMigrations": "@{invocations.migration.grouped.migrations}"
        }
    },
    "migration.service.factory": {
        "service": "services.core.service.manager",
        "method": "getServiceFactory"
    }
}